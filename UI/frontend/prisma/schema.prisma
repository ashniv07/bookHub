generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}
model auth{
  user_id Int  @id @default(autoincrement())
  user_name String  @db.VarChar(50)
  password  String  @db.VarChar(30)
  role  Int
}
